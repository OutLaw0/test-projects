		"autoprefixer": "^10.4.21",
		"postcss": "^8.5.6",
		"svelte": "4.2.20",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.0.0",
		"vite": "4.5.14"
	},
	"dependencies": {
		"@svelteuidev/composables": "^0.15.7",
		"@svelteuidev/core": "^0.15.7"
	}
}
